  "scripts": {
    "setup-project": "npm install",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "prestorybook": "set CI=true && react-app-rewired test --json --outputFile=.jest-test-results.json",
    "storybook": "start-storybook -s ../BT_WebApp -p 6006",
    "storybook:tests": "react-app-rewired test --json --outputFile=.jest-test-results.json",
    "storybook:kill": "for /f \"tokens=5\" %a in ('netstat -aon ^| find \":6006\" ^| find \"LISTENING\"') do taskkill /f /pid %a",
    "prestorybook-deploy": "set CI=true && react-app-rewired test --json --outputFile=.jest-test-results.json",
    "storybook-deploy": "build-storybook -c .storybook -o ../BT_WebApp/styleguide"
